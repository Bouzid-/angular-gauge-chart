{
  "name": "angular-gauge-chart",
  "version": "0.4.0",
  "description": "Angular Gauge Chart",
  "keywords": ["angular", "angular2", "webpack", "typescript"],
  "main": "dist/bundle.js",
  "author": "",
  "license": "",
  "scripts": {
    "build": "npm run webpack -- --config config/webpack.config.js",
    "build:dev":
      "npm run clean:dist && npm run webpack -- --config examples/config/webpack.dev.js --progress --profile",
    "clean:dist": "npm run rimraf -- dist",
    "clean":
      "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "docs":
      "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "rimraf": "rimraf",
    "server:dev":
      "npm run webpack-dev-server -- --config examples/config/webpack.dev.js --open --progress --profile --watch --content-base examples/",
    "start": "npm run server:dev",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "watch": "npm run watch:dev",
    "webpack-dev-server":
      "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack":
      "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@angular/core": "~4.0.3",
    "gauge-chart":
      "git+ssh://git@bitbucket.org/recogizer/gauge-chart.git#v0.4.0"
  },
  "peerDependencies": {
    "@angular/core": "~4.0.3",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@angular/animations": "^4.4.7",
    "@angular/common": "~4.0.3",
    "@angular/compiler": "~4.0.3",
    "@angular/compiler-cli": "~4.0.3",
    "@angular/forms": "~4.0.3",
    "@angular/http": "~4.0.3",
    "@angular/platform-browser": "~4.0.3",
    "@angular/platform-browser-dynamic": "~4.0.3",
    "@angular/platform-server": "~4.0.3",
    "@angular/router": "~4.0.3",
    "@angularclass/conventions-loader": "^1.0.13",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "2.5.45",
    "@types/node": "^7.0.64",
    "@types/source-map": "^0.5.7",
    "@types/uglify-js": "^2.6.31",
    "@types/webpack": "^2.2.15",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "~3.1.2",
    "codelyzer": "~2.1.1",
    "core-js": "^2.5.6",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "^2.99.1",
    "json-loader": "^0.5.4",
    "lint-staged": "^7.1.0",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "~2.0.0",
    "node-sass": "^4.9.0",
    "prettier": "^1.12.1",
    "protractor": "^5.3.2",
    "raw-loader": "0.5.1",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.10",
    "sass-loader": "^6.0.7",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "to-string-loader": "^1.1.5",
    "tslib": "^1.9.1",
    "tslint": "~4.5.1",
    "typedoc": "^0.11.1",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "^4.1.2",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5"
  },
  "lint-staged": {
    "*.{js,json,css}": ["prettier --write", "git add"],
    "*.ts": [
      "prettier --write",
      "tslint --fix -c ./tslint.json 'src/**/*.ts'",
      "git add"
    ]
  }
}
