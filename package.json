{
  "name": "@recogizer/angular-gauge-chart",
  "version": "0.4.1",
  "description": "Angular Gauge Chart",
  "keywords": [
    "angular",
    "angular2",
    "webpack",
    "typescript"
  ],
  "main": "dist/bundle.js",
  "author": "“Maxim Maltsev” <“maltsevmn@ya.ru”>, “Alexey Karpov” <“karpovad@yandex.ru”>",
  "license": "MIT",
  "scripts": {
    "build": "npm run webpack -- --config config/webpack.config.js",
    "build:dev": "npm run clean:dist && npm run webpack -- --config examples/config/webpack.dev.js --progress --profile",
    "clean:dist": "npm run rimraf -- dist",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "rimraf": "rimraf",
    "server:dev": "npm run webpack-dev-server -- --config examples/config/webpack.dev.js --open --progress --profile --watch --content-base examples/",
    "start": "npm run server:dev",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc --out docs/",
    "watch": "npm run watch:dev",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
    "@angular/core": "~4.0.3",
    "@recogizer/gauge-chart": "^0.4.3"
  },
  "peerDependencies": {
    "@angular/core": "~4.0.3",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@angular/animations": "^4.0.3",
    "@angular/common": "~4.0.3",
